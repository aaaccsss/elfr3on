{
  "version": 2,
  "builds": [
    { "src": "user.js", "use": "@vercel/node" },
    { "src": "avatar.js", "use": "@vercel/node" },
    { "src": "status.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/support-notifications", "dest": "/user.js" },
    { "src": "/api/user/(.*)/avatar", "dest": "/avatar.js" },
    { "src": "/api/user/(.*)", "dest": "/user.js" },
    { "src": "/api/status", "dest": "/status.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
